{
	"name": "Tab Capture",
	"version": "1.0",
	"description": "Organize and store browsing tabs",
	"permissions": [
			"activeTab",
			"tabs",
			"storage",
			"bookmarks"
			],

	"browser_action": {
		"default_popup": "tabcap.html",
		"default_title": "TabCapture"
	},

	"icons": { "128": "tabCapture-128.png"
	},

	"manifest_version": 2
}
